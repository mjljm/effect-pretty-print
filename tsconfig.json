{
	"extends": ["./tsconfig-base-library.json"],
	"compilerOptions": {
		// This path definition allows auto-import in VSCode with
		// "typescript.preferences.importModuleSpecifier": "non-relative",
		"paths": {
			"@mjljm/effect-pretty-print/*": ["./src/*.ts"]
		},
		"plugins": [
			{
				"name": "@effect/language-service"
			}
		]
	},
	"include": [".*", "*", "src"]
}
